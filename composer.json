{
    "name": "elsayed85/notion",
    "description": "This is my package Notion",
    "keywords": [
        "elsayed85",
        "laravel",
        "notion"
    ],
    "homepage": "https://github.com/elsayed85/notion",
    "license": "MIT",
    "authors": [
        {
            "name": "elsayed85",
            "email": "elsayedkamal581999@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Elsayed85\\Notion\\": "src",
            "Elsayed85\\Notion\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elsayed85\\Notion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elsayed85\\Notion\\NotionServiceProvider"
            ],
            "aliases": {
                "Notion": "Elsayed85\\Notion\\NotionFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
